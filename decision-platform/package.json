{
  "name": "decision-platform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:check": "eslint . --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "docker:dev": "docker-compose -f docker-compose.minimal.yml up -d",
    "docker:staging": "docker-compose -f docker-compose.staging.yml up -d",
    "docker:prod": "docker-compose -f docker-compose.production.yml up -d",
    "docker:down": "docker-compose down --remove-orphans",
    "docker:clean": "docker system prune -af && docker volume prune -f",
    "security:audit": "pnpm audit --audit-level high",
    "security:fix": "pnpm audit --fix",
    "precommit": "pnpm lint:check && pnpm type-check && pnpm test:ci",
    "prepare": "husky install"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/pg": "^8.10.0",
    "bullmq": "^5.4.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.562.0",
    "minio": "^8.0.0",
    "next": "16.0.10",
    "pg": "^8.11.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "tailwind-merge": "^3.4.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.10",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^16.2.7",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
