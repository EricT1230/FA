# ========================================
# Staging Environment Configuration
# ========================================

# Environment
NODE_ENV=staging
STAGING_DOMAIN=staging.fa.example.com

# Database
DB_USER=fa_user
DB_PASSWORD=your-secure-db-password-staging
DATABASE_URL=postgresql://fa_user:your-secure-db-password-staging@db:5432/freelancer_aggregator_staging

# Redis
REDIS_URL=redis://redis:6379

# MinIO Storage
MINIO_ACCESS_KEY=staging-minio-access-key
MINIO_SECRET_KEY=your-secure-minio-secret-staging
MINIO_ENDPOINT=http://minio:9000

# Authentication & Security
JWT_SECRET=your-jwt-secret-staging-change-this
SESSION_SECRET=your-session-secret-staging-change-this
NEXTAUTH_SECRET=your-nextauth-secret-staging-change-this
NEXTAUTH_URL=https://staging.fa.example.com

# OAuth Providers (Staging Apps)
GOOGLE_CLIENT_ID=your-google-client-id-staging
GOOGLE_CLIENT_SECRET=your-google-client-secret-staging
GITHUB_CLIENT_ID=your-github-client-id-staging
GITHUB_CLIENT_SECRET=your-github-client-secret-staging

# N8N Configuration
N8N_USER=admin
N8N_PASSWORD=your-n8n-password-staging
N8N_ENCRYPTION_KEY=your-n8n-encryption-key-staging
N8N_SECURE_COOKIE=false
N8N_DISABLE_UI=false

# Monitoring & Logging
LOG_LEVEL=info
SENTRY_DSN=your-sentry-dsn-staging

# Email Configuration (for N8N notifications)
SMTP_HOST=smtp.example.com
SMTP_PORT=587
SMTP_USER=your-smtp-user
SMTP_PASSWORD=your-smtp-password
SMTP_FROM=no-reply@staging.fa.example.com

# External Service URLs
FA_APP_URL=https://staging.fa.example.com
FA_AUTH_URL=https://staging.fa.example.com/auth
FA_N8N_URL=https://staging.fa.example.com/workflows

# Performance Settings
WORKER_CONCURRENCY=3
MAX_REQUEST_SIZE=10mb
CACHE_TTL=300

# Feature Flags
ENABLE_TELEMETRY=true
ENABLE_DEBUG_MODE=true
ENABLE_DEVELOPMENT_TOOLS=true